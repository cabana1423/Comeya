<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <TextView
        android:id="@+id/VistaRest_nombre"
        android:layout_width="388dp"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="7dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/VistaRest_telefono"
        android:layout_width="152dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/VistaRest_nit"
        android:layout_alignParentStart="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="8dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/VistaRest_nit"
        android:layout_width="153dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/VistaRest_propietario"
        android:layout_alignParentStart="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="6dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/VistaRest_propietario"
        android:layout_width="153dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Vistarest_img"
        android:layout_alignParentStart="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="17dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/VistaRest_direccion"
        android:layout_width="152dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/VistaRest_telefono"
        android:layout_alignParentStart="true"
        android:layout_marginStart="36dp"
        android:layout_marginTop="10dp"
        android:text="TextView" />

    <ImageView
        android:id="@+id/Vistarest_img"
        android:layout_width="348dp"
        android:layout_height="151dp"
        android:layout_below="@+id/VistaRest_nombre"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="31dp"
        android:src="@drawable/image_defect" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/VistaRest_mapa"
        android:layout_width="326dp"
        android:layout_height="148dp"
        android:layout_below="@+id/VistaRest_direccion"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="45dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="40dp" />

</RelativeLayout>